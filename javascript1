<body>
    <script>
        let age = window.prompt("What is your age?");

        // Calculates an age rage between 14 and 90
        // if (age <= 14 || age >= 90) {
        //     alert("You are within age range");
        // }
        // else {
        //     alert("You are outside the age range!");
        // }


        // using the !(Not) Operator to do the same calculation above
        if (!(age >= 14 && age <= 90)) {
            alert("You are within age range");
        }
        else {
            alert("You are not within the age range!");
        }


    </script>
</body>
