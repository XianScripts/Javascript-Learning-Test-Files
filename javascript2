<body>
    <script>
        //Prompt user for login name
         let welcome = window.prompt("Please enter your login name");
        // Checks for correct username from user
        if (welcome === "Admin") {
            let password = window.prompt("What is the password?");
            if (password === "TheMaster") {
                alert("Welcome");
            }
            else if (password === "") {
                alert("Canceled");
            }
            else {
                alert("Wrong password");
            }
            
        else if (welcome === ""){
            alert("Canceled");
        }
        else {
            alert("I don't know you");
        }

    </script>
</body>
